{
  "name": "araguaney",
  "version": "1.0.0",
  "description": "Backend API para el juego diario de palabras venezolanas Criollazo",
  "main": "dist/config/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/config/app.ts",
    "build": "tsc",
    "start": "node dist/config/app.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": ["venezuela", "wordle", "game", "api", "express", "typescript"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@vitest/ui": "^3.2.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
